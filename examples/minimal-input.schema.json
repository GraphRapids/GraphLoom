{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/Faerkeren/ElkPydantic/examples/minimal-input.schema.json",
  "description": "Schema for ElkPydantic minimal graph input.",
  "$defs": {
    "MinimalEdgeIn": {
      "properties": {
        "name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Name"
        },
        "type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Type"
        },
        "from": {
          "title": "From",
          "type": "string"
        },
        "to": {
          "title": "To",
          "type": "string"
        }
      },
      "required": [
        "from",
        "to"
      ],
      "title": "MinimalEdgeIn",
      "type": "object"
    },
    "MinimalNodeIn": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Type"
        },
        "id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Id"
        },
        "nodes": {
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/MinimalNodeIn"
              },
              {
                "type": "string"
              }
            ]
          },
          "title": "Nodes",
          "type": "array"
        },
        "edges": {
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/MinimalEdgeIn"
              },
              {
                "type": "string"
              }
            ]
          },
          "title": "Edges",
          "type": "array"
        }
      },
      "required": [
        "name"
      ],
      "title": "MinimalNodeIn",
      "type": "object"
    }
  },
  "properties": {
    "nodes": {
      "items": {
        "anyOf": [
          {
            "$ref": "#/$defs/MinimalNodeIn"
          },
          {
            "type": "string"
          }
        ]
      },
      "title": "Nodes",
      "type": "array"
    },
    "edges": {
      "items": {
        "anyOf": [
          {
            "$ref": "#/$defs/MinimalEdgeIn"
          },
          {
            "type": "string"
          }
        ]
      },
      "title": "Edges",
      "type": "array"
    }
  },
  "required": [
    "nodes"
  ],
  "title": "MinimalGraphIn",
  "type": "object"
}
